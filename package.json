{
  "name": "mccustom-starter",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "eslint": "^5.3.0",
    "node-sass": "^4.9.3",
    "nunjucks": "^3.1.3",
    "parcel-bundler": "^1.10.2"
  },
  "config": {
    "url": "[MEDIA SERVER PATH]",
    "docID": "[GOOGLE DOC ID GOES HERE]"
  },
  "scripts": {
    "start": "parcel src/html/index.njk",
    "cue": "parcel build src/html/index.njk --public-url $npm_package_config_url$npm_package_name --out-dir cue",
    "text": "googledoc-to-json $npm_package_config_docID -c drive.json -o ./src/html/context.json"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "parcel-plugin-nunjucks": "git+https://git@github.com/aaronalbright/parcel-plugin-nunjucks.git"
  }
}
